{
  "name": "escolalms/api",
  "description": "Headless LMS",
  "type": "project",
  "keywords": [
    "lms",
    "learning-management-system",
    "laravel",
    "reactjs"
  ],
  "homepage": "https://lms.escolasoft.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Mateusz Wojczal",
      "email": "mateusz@wojczal.com"
    },
    {
      "name": "Paweł Chołaj",
      "email": "pawel.cholaj@escola.pl",
      "homepage": "https://cholaj.pl"
    }
  ],
  "require": {
    "php": ">=7.4",
    "doctrine/dbal": "^2|^3",
    "escolalms/auth": "^0",
    "escolalms/cart": "0.1.19",
    "escolalms/categories": "^0",
    "escolalms/core": "^1",
    "escolalms/courses": "^0",
    "escolalms/courses-import-export": "^0",
    "escolalms/csv-users": "^0",
    "escolalms/files": "^0",
    "escolalms/headless-h5p": "^0",
    "escolalms/images": "^0",
    "escolalms/lrs": "^0",
    "escolalms/mailerlite": "^0",
    "escolalms/mattermost": "^0",
    "escolalms/notifications": "^0",
    "escolalms/pages": "^0",
    "escolalms/payments": "^0",
    "escolalms/permissions": "^0",
    "escolalms/reports": "^0",
    "escolalms/scorm": "^0",
    "escolalms/settings": "^0",
    "escolalms/stationary-events": "^0",
    "escolalms/tags": "^0",
    "escolalms/templates": "^0",
    "escolalms/templates-email": "^0",
    "escolalms/templates-pdf": "^0",
    "escolalms/templates-sms": "^0",
    "escolalms/topic-types": "^0",
    "escolalms/questionnaire": "^0",
    "escolalms/consultations": "^0",
    "escolalms/assign-without-account": "^0",
    "escolalms/video": "^0",
    "fideloper/proxy": "^4.0",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.3",
    "laravel/framework": "^8",
    "spatie/laravel-permission": "^3.18"
  },
  "require-dev": {
    "based/laravel-typescript": "^0.0.2",
    "facade/ignition": "^2.0",
    "fakerphp/faker": "^1.9.1",
    "filp/whoops": "^2.0",
    "nunomaduro/collision": "^5.0",
    "phpunit/phpunit": "^9.0"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "hooks": {
      "pre-commit": [
        "echo committing as $(git config user.name)",
        "php-cs-fixer fix ."
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "siffer": [
      "./vendor/bin/php-cs-fixer fix ."
    ],
    "behat": [
      "./vendor/bin/behat"
    ]
  },
  "repositories": {}
}
