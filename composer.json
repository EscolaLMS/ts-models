{
  "name": "escolalms/api",
  "description": "Headless LMS",
  "type": "project",
  "keywords": [
    "lms",
    "learning-management-system",
    "laravel",
    "reactjs"
  ],
  "homepage": "https://lms.escolasoft.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Mateusz Wojczal",
      "email": "mateusz@wojczal.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "doctrine/dbal": "^2|^3",
    "escolalms/auth": "^0",
    "escolalms/cart": "0.1.19",
    "escolalms/categories": "^0",
    "escolalms/core": "^1",
    "escolalms/courses": "^0",
    "escolalms/courses-import-export": "^0",
    "escolalms/csv-users": "^0",
    "escolalms/files": "^0",
    "escolalms/headless-h5p": "^0",
    "escolalms/images": "^0",
    "escolalms/lrs": "^0",
    "escolalms/mailerlite": "^0",
    "escolalms/mattermost": "^0",
    "escolalms/notifications": "^0",
    "escolalms/pages": "^0",
    "escolalms/payments": "^0",
    "escolalms/permissions": "^0",
    "escolalms/reports": "^0",
    "escolalms/scorm": "^0",
    "escolalms/settings": "^0",
    "escolalms/stationary-events": "^0",
    "escolalms/tags": "^0",
    "escolalms/templates": "^0",
    "escolalms/templates-email": "^0",
    "escolalms/templates-pdf": "^0",
    "escolalms/templates-sms": "^0",
    "escolalms/topic-types": "^0",
    "escolalms/questionnaire": "^0",
    "escolalms/consultations": "^0",
    "escolalms/assign-without-account": "^0",
    "escolalms/video": "^0",
    "laravel/framework": "^8.0",
    "laravel/helpers": "^1.4",
    "laravel/horizon": "^5.7",
    "laravel/passport": "^10.0",
    "laravel/socialite": "^5.1",
    "laravel/tinker": "^2.5",
    "laravel/ui": "^3.0",
    "predis/predis": "^1.1",
    "sentry/sentry-laravel": "^2.3",
    "spatie/laravel-permission": "^3.18"
  },
  "require-dev": {
    "based/laravel-typescript": "^0.0.2",
    "orchestra/testbench": "^6.17"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "composer/package-versions-deprecated": true
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "hooks": {
      "pre-commit": [
        "echo committing as $(git config user.name)",
        "php-cs-fixer fix ."
      ]
    }
  },
  "autoload": {
    "psr-4": {}
  },
  "autoload-dev": {
    "psr-4": {}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {}
}
